/* pages/personal/card/card_list.wxss */
.container{background: #eee;height: 100vh}

.header{background: #fff;padding: 3vw 4vw;display: flex;justify-content: space-between;align-items: center;font-size: 28rpx;}
.bindCard{color: #fff;background: rgba(148, 71, 15, 0.842);padding: 0 20rpx;border-radius: 8rpx;line-height: 2.4em}

.list-wrapper{padding: 3vw;}

.card{background: #fff;padding: 4vw;}
.item{width: 100%;display: flex;justify-content: space-between;font-size: 28rpx;padding:20rpx 0; }
.vip{border: 1px solid rgba(148, 71, 15, 0.842);color: rgba(148, 71, 15, 0.842);border-radius: 16rpx;line-height: 1m;padding: 0 16rpx;border-left: none;margin-left: 20rpx;}
.crown{display: in;background:rgba(148, 71, 15, 0.842);width: 40rpx;height: 40rpx;line-height: 20rpx;text-align: center; }
.icon-huangguanguanjun{color: #eee;}
.cancelBind{border: 1px solid rgba(148, 71, 15, 0.842);color: rgba(148, 71, 15, 0.842);border-radius: 8rpx;line-height: 1.5em;padding: 0 16rpx;}